    box-sizing: border-box;
   /* display: inline-block;  */

}

.content .box .imgbox{
    position: relative;
   overflow: hidden; 
}
.content .box .imgbox img{
    width: 250px;
    height: 300px;
    transition: transform 2s;
}  
.content .box:hover .imgbox img{
    transform: scale(1.1);

}
.content .box .text{
    position: absolute;
    width: 220px;
    height: 280px;
    left: 10px;
    right: 10px;
    bottom: 10px;
    top: 10px;
    background-image:url("F5_Images/BG7.jpg");
    transform:scaleY(0);
    transition: transform  2s;
}

.content .box:hover .text{
    transform: scaleY(1);

