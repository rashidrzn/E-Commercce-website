							<input type="checkbox" name="bat" id="batId" onclick="batCartSelect();">
							<img src="../F5_Images/cart.png" alt="Cart Image" width="20px" height="20px">
							
						<div id="batPrice"></div>
							
					</div>
					
					<!-- -------------------- [3] HEAD WEARS -------------------- -->
					
					<div class="ProductHeadWears">
						<img src="../F5_Images/helmet.png" alt="Helmet Image" width="120px" height="120px">
						<br>
						
