		<form novalidate> 
			
			<fieldset class="personal_F">
				<legend id="personal_D">Personal Details</legend>
				
				<label for="nameId">Name: </label>
					<input type="text" name="name" id="nameId" placeholder="Enter Your Name">
					<br><br>
					
				<label for="emailId">Email: </label>
					<input type="email" name="email" id="emailId" placeholder="Enter Your Email Address">
					<br><br>
				
				<label for="addressId">Address:</label>
					<textarea name="address" id="addressId" rows="1" cols="40" placeholder="Enter Your Home Address"></textarea>
					<br><br>
					
				<label for="telId">Mobile No: </label>
					<input type="tel" name="tel" id="telId" placeholder="Enter Your Phone Number">
					<br><br>
			</fieldset>
			
			<fieldset class="product_F">
				<legend id="product_D">Product Details</legend>
				
					<!-- -------------------- [1] BAGS & LUGGAGES -------------------- -->
				
					<div class="ProductBag">
						<img src="../F5_Images/bag.png" alt="Bag Image" width="120px" height="120px">
						<br>
						
						<label class="Products">BAGS & LUGGAGES</label>
						<br><br>
						
						<label for="bagTypeId">Type:</label>
						<input name="bagType" list="bagTypeList" id="bagTypeId">
						<datalist id="bagTypeList">
							<option value="Bat Cover">
							<option value="Wheelie Bag">
							<option value="Duffle Bag">
						</datalist>
						<br><br>
						
						<label for="bagBrandId">Brand:</label>
						<input name="bagBrand" list="bagBrandList" id="bagBrandId">
						<datalist id="bagBrandList">
							<option value="Gunn & Moore (GM)">
							<option value="Kookaburra">
							<option value="New Balance">
						</datalist>
						<br><br>
						
						<label for="bagQuantityId">Quantity:</label>
							<input type="number" name="bagQuantity" id="bagQuantityId" min="0" max="100">
						<br><br>
						
						<label class="addTo" for="bagId">Add to cart</label>
							<input type="checkbox" name="bag" id="bagId" onclick="bagCartSelect();">
							<img src="../F5_Images/cart.png" alt="Cart Image" width="20px" height="20px">
							
						<div id="bagPrice"></div>
							
					</div>
					
					<!-- -------------------- [2] BATS -------------------- -->
			
					<div class="ProductBat">
						<img src="../F5_Images/bat.png" alt="Bat Image" width="110px" height="220px">
						<br>
						
						<label class="Products">BATS</label>
						<br><br>
						
						<label for="batTypeId">Type:</label>
						<input name="batType" list="batTypeList" id="batTypeId">
						<datalist id="batTypeList">
							<option value="Size 1">
							<option value="Size 2">
							<option value="Size 3">
						</datalist>
						<br><br>
						
						<label for="batBrandId">Brand:</label>
						<input name="batBrand" list="batBrandList" id="batBrandId">
						<datalist id="batBrandList">
							<option value="MRF">
							<option value="Kookaburra">
							<option value="SS">
						</datalist>
						<br><br>
						
						<label for="batQuantityId">Quantity:</label>
							<input type="number" name="batQuantity" id="batQuantityId" min="0" max="100">
						<br><br>
						
